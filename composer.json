{
    "name": "jpcaparas/laravel-faker-openai",
    "description": "An opinionated Laravel package that modifies the service container Faker behaviour to allow prompts to be generated with OpenAI",
    "license": "MIT",
    "authors": [
        {
            "name": "JP Caparas",
            "email": "jp@jpcaparas.com"
        }
    ],
    "type": "library",
    "autoload": {
        "psr-4": {
            "JPCaparas\\FakerOpenAI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JPCaparas\\FakerOpenAI\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "fakerphp/faker": "^1.24",
        "laravel/framework": "^10.0 || ^11.0 || ^12.0",
        "nyholm/psr7": "^1.8",
        "openai-php/laravel": "^0.10.2",
        "symfony/http-client": "^7.2"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "laravel/pint": "^1.19",
        "orchestra/testbench": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JPCaparas\\FakerOpenAI\\FakerOpenAIServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "pint",
        "tests": "phpunit",
        "tinker": "testbench tinker"
    }
}
